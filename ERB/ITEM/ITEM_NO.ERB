@ITEM_NO_CONSUMABLES(ARG)
    #FUNCTION
    #DIM ITEM_NO
    SELECTCASE ARG
        CASE 1 TO 99
            ITEM_NO = ARG + 100
            RETURNF ITEM_NO
    ENDSELECT

