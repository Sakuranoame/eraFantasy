@UI_GAL
    CALL UI_LINE
    CALL UI_GAL_BUTTON
    CALL UI_GAL_BUTTON_GROUP




@UI_GAL_BUTTON_GROUP
    #DIM LCOUNT 
    FOR LCOUNT, 0, 6
        IF INDEX(UI_GAL_BUTTON_GROUP:LCOUNT) == GAL按钮类别选择
            CALL PRINT_BUTTON_SELECTED(UI_GAL_BUTTON_GROUP:LCOUNT, 8)
        ELSE
            CALL PRINT_BUTTON(UI_GAL_BUTTON_GROUP:LCOUNT, 8, LCOUNT)
        ENDIF
    NEXT


@UI_GAL_BUTTON
    #DIM LCOUNT
    SELECTCASE GAL按钮类别选择
        CASE INDEX("互动")
            UI_GAL_BUTTON:0 = 对话
            UI_GAL_BUTTON:1 = 抱抱
        CASE INDEX("交互")
    ENDSELECT
    FOR LCOUNT, 0 ,10
        CALL PRINT_BUTTON_1(UI_GAL_BUTTON:LCOUNT, 8, LCOUNT + 10)
    NEXT
    PRINTL
    CUSTOMDRAWLINE -