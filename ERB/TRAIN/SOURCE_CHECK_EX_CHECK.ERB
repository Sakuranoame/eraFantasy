@SOURCE_CHECK_EX_CHECK; 高潮运算
    #DIM LCOUNT
    #DIMS STOP_EX
    #DIMS ENDURE_EX
    #DIM EXLEVEL = 0
    FOR LCOUNT, 0, 10
        SELECTCASE NOW_PALAM(LCOUNT)
            CASE IS >= 200000
                NOWEX:LCOUNT = 5
            CASE IS >= 100000
                NOWEX:LCOUNT = 4
            CASE IS >= 50000
                NOWEX:LCOUNT = 3
            CASE IS >= 20000
                NOWEX:LCOUNT = 2
            CASE IS >= 10000
                NOWEX:LCOUNT = 1
            CASE IS < 10000
                NOWEX:LCOUNT = 0
        ENDSELECT
        SIF NOWEX:LCOUNT > 0
            TFLAG:今回高潮 = 1
    NEXT
    IF TFLAG:忍耐中 == 1
        FOR LCOUNT, 0, 10
            EXLEVEL += NOWEX:LCOUNT
        NEXT
        IF EXLEVEL*2 <= ABL:TARGET:快感忍耐*7
            IF RAND:10 > 8
                FOR LCOUNT, 0, 10
                    NOWEX:LCOUNT = 0
                NEXT
            ELSE
                TFLAG:忍耐中 = 0
                TFLAG:忍耐失败 = 1
                TFLAG:今回高潮 = 1
            ENDIF
        ELSE
            IF RAND:10 > 4
                FOR LCOUNT, 0, 10
                    NOWEX:LCOUNT = 0
                NEXT
            ELSE
                TFLAG:忍耐中 = 0
                TFLAG:忍耐失败 = 1
                TFLAG:今回高潮 = 1
            ENDIF
        ENDIF
    ENDIF

@NOW_PALAM(ARG)
#FUNCTION
LOCAL = SOURCE:TARGET:ARG + PALAM:TARGET:(100 + ARG)
RETURNF LOCAL