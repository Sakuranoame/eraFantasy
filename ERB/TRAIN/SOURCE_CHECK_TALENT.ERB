@SOURCE_CHECK_TALENT ;特质计算
    ;钝感
    SIF TALENT:TARGET:M性感 == 1
        TIMES SOURCE:快M, 0.8
    SIF TALENT:TARGET:C性感 == 1
        TIMES SOURCE:快C, 0.8
    SIF TALENT:TARGET:V性感 == 1
        TIMES SOURCE:快V, 0.8
    SIF TALENT:TARGET:A性感 == 1
        TIMES SOURCE:快A, 0.8
    SIF TALENT:TARGET:B性感 == 1
        TIMES SOURCE:快B, 0.8
    SIF TALENT:TARGET:尿道性感 == 1
        TIMES SOURCE:快U, 0.8
    ;性感
    SIF TALENT:TARGET:M性感 == 2
        TIMES SOURCE:快M, 1.1
    SIF TALENT:TARGET:C性感 == 2
        TIMES SOURCE:快C, 1.1
    SIF TALENT:TARGET:V性感 == 2
        TIMES SOURCE:快V, 1.1
    SIF TALENT:TARGET:A性感 == 2
        TIMES SOURCE:快A, 1.1
    SIF TALENT:TARGET:B性感 == 2
        TIMES SOURCE:快B, 1.1
    SIF TALENT:TARGET:尿道性感 == 2
        TIMES SOURCE:快U, 1.1
    ;过敏
    SIF TALENT:TARGET:M性感 == 3
        TIMES SOURCE:快M, 1.25
    SIF TALENT:TARGET:C性感 == 3
        TIMES SOURCE:快C, 1.25
    SIF TALENT:TARGET:V性感 == 3
        TIMES SOURCE:快V, 1.25
    SIF TALENT:TARGET:A性感 == 3
        TIMES SOURCE:快A, 1.25
    SIF TALENT:TARGET:B性感 == 3
        TIMES SOURCE:快B, 1.25
    SIF TALENT:TARGET:尿道性感 == 3
        TIMES SOURCE:快U, 1.25

    