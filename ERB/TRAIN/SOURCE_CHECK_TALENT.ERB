@SOURCE_CHECK_TALENT ;特质计算
    ;钝感
    SIF TALENT:TARGET:Ｍ钝感 == 1
        TIMES SOURCE:快Ｍ, 0.8
    SIF TALENT:TARGET:Ｃ钝感 == 1
        TIMES SOURCE:快Ｃ, 0.8
    SIF TALENT:TARGET:Ｖ钝感 == 1
        TIMES SOURCE:快Ｖ, 0.8
    SIF TALENT:TARGET:Ａ钝感 == 1
        TIMES SOURCE:快Ａ, 0.8
    SIF TALENT:TARGET:Ｂ钝感 == 1
        TIMES SOURCE:快Ｂ, 0.8
    SIF TALENT:TARGET:Ｕ钝感 == 1
        TIMES SOURCE:快Ｕ, 0.8
    ;性感
    SIF TALENT:TARGET:Ｍ性感 == 1
        TIMES SOURCE:快Ｍ, 1.1
    SIF TALENT:TARGET:Ｃ性感 == 1
        TIMES SOURCE:快Ｃ, 1.1
    SIF TALENT:TARGET:Ｖ性感 == 1
        TIMES SOURCE:快Ｖ, 1.1
    SIF TALENT:TARGET:Ａ性感 == 1
        TIMES SOURCE:快Ａ, 1.1
    SIF TALENT:TARGET:Ｂ性感 == 1
        TIMES SOURCE:快Ｂ, 1.1
    SIF TALENT:TARGET:Ｕ性感 == 1
        TIMES SOURCE:快Ｕ, 1.1
    ;过敏
    SIF TALENT:TARGET:Ｍ过敏 == 1
        TIMES SOURCE:快Ｍ, 1.25
    SIF TALENT:TARGET:Ｃ过敏 == 1
        TIMES SOURCE:快Ｃ, 1.25
    SIF TALENT:TARGET:Ｖ过敏 == 1
        TIMES SOURCE:快Ｖ, 1.25
    SIF TALENT:TARGET:Ａ过敏 == 1
        TIMES SOURCE:快Ａ, 1.25
    SIF TALENT:TARGET:Ｂ过敏 == 1
        TIMES SOURCE:快Ｂ, 1.25
    SIF TALENT:TARGET:Ｕ过敏 == 1
        TIMES SOURCE:快Ｕ, 1.25

    