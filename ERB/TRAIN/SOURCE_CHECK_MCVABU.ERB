        SOURCE:快C = SOURCE:快C *(100 + ABL:TARGET:C感觉*100) / 100
    SIF ABL:TARGET:V感觉 >0
        SOURCE:快V = SOURCE:快V *(100 + ABL:TARGET:V感觉*100) / 100
    SIF ABL:TARGET:A感觉 >0
        SOURCE:快A = SOURCE:快A *(100 + ABL:TARGET:A感觉*100) / 100
    SIF ABL:TARGET:B感觉 >0
        SOURCE:快B = SOURCE:快B *(100 + ABL:TARGET:B感觉*100) / 100
    SIF ABL:TARGET:尿道感觉 >0
        SOURCE:快U = SOURCE:快U *(100 + ABL:TARGET:U感觉*100) / 100