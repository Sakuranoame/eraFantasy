@SOURCE_CHECK_LUST
    #DIM LCOUNT
    ;性欲加成
    IF BASE:TARGET:5 <= (20*MAXBASE:TARGET:5) / 100
        FOR LCOUNT, 1, 10
            TIMES SOURCE:LCOUNT, 0.9
        NEXT
        TIMES SOURCE:欲情, 0.8
    ELSEIF BASE:TARGET:5 >= (40*MAXBASE:TARGET:5) / 100 & BASE:TARGET:5 <= (60*MAXBASE:TARGET:5) / 100
        FOR LCOUNT, 1, 10
            TIMES SOURCE:LCOUNT, 1.1
        NEXT
        TIMES SOURCE:欲情, 0.9
    ELSEIF BASE:TARGET:5 >= (60*MAXBASE:TARGET:5) / 100 & BASE:TARGET:5 <= (80*MAXBASE:TARGET:5) / 100
        FOR LCOUNT, 1, 10
            TIMES SOURCE:LCOUNT, 1.2
        NEXT
        TIMES SOURCE:欲情, 1.1
    ELSEIF BASE:TARGET:5 >= (80*MAXBASE:TARGET:5) / 100 & BASE:TARGET:5 <= (99*MAXBASE:TARGET:5) / 100
        FOR LCOUNT, 1, 10
            TIMES SOURCE:LCOUNT, 1.5
        NEXT
        TIMES SOURCE:欲情, 1.2
    ELSEIF BASE:TARGET:5 > (99*MAXBASE:TARGET:5) / 100
        FOR LCOUNT, 1, 10
            TIMES SOURCE:LCOUNT, 2.0
        NEXT
        TIMES SOURCE:欲情, 1.5
    ENDIF
    