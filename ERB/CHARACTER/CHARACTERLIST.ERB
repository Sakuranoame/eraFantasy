@CHARACTER_LIST, ARG ;角色列表 1=显示能力,2=能力提升
    #DIM CHARACTERNUM
    #DIM NUM
    NUM = 0
    CALL DRAWLINES("□　角色列表　□")
    FOR CHARACTERNUM, 1, 100
        NUM += 1
        IF EXISTCSV(CHARACTERNUM-1,0) == 1  
            PRINTFORML [{NUM}] %NAME:NUM%
        ENDIF
    NEXT
    CUSTOMDRAWLINE -
    PRINTFORML [99] 返回
    INPUT
    ;IF EXISTCSV(RESULT,0) == 1
    SIF RESULT != 99
        CALL CHARACTER_LIST_CHECK, RESULT ,ARG
    ;ENDIF

@OTHER_CHARACTER_LIST, ARG ;没有主角 1=切换调教对象，2=更换助手，3=放置调教，4=管理角色
    #DIM CHARACTERNUM
    #DIM NUM
    NUM = 1
    CALL DRAWLINES("□　角色列表　□")
    FOR CHARACTERNUM, 2, 100
        NUM += 1
        IF EXISTCSV(CHARACTERNUM-1,0) == 1
            PRINTFORML [{NUM}] %NAME:NUM%
        ENDIF
    NEXT
    PRINTFORML [99] 返回
    INPUT
    IF EXISTCSV(RESULT-1,0) == 1
        CALL OTHER_CHARACTER_LIST_CHECK, RESULT, ARG
    ENDIF