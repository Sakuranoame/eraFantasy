@CHARACTER_LIST_CHECK, ARG, ARG:1
    IF ARG:1 == 1   
        CALL ABILITY_SHOW(ARG,1)
    ELSEIF ARG:1 == 2
        CALL PRINT_SHOW_ABLUP_SELECT(ARG)
    ENDIF

@OTHER_CHARACTER_LIST_CHECK, ARG, ARG:1
    IF ARG:1 == 1
        TARGET = ARG
    ELSEIF ARG:1 == 2
        ASSI = ARG
    ELSEIF ARG:1 == 3   
        CALL PLACETRAIN, ARG
    ENDIF