@SHOW_JUEL

@SHOW_ABLUP_SELECT
    CALL PRINT_SHOW_ABLUP_SELECT(TARGET)


@PRINT_SHOW_ABLUP_SELECT(ARG)
    #DIM LCOUNT
    #DIM PCOUNT = 0
    ALIGNMENT CENTER
    IF ARG != MASTER
        CALL DRAWLINES_CENTER("【　感觉　】")
        FOR LCOUNT, 0, 20
            IF ABLNAME:LCOUNT != ""
                PRINTBUTTONLC @"%ABLNAME:LCOUNT%LV:{ABL:ARG:LCOUNT}", LCOUNT
                PCOUNT += 1
            ENDIF
            IF PCOUNT >= 5
                PRINTL
                PCOUNT = 0
            ENDIF
        NEXT
        PRINTL
        PCOUNT = 0
        CALL DRAWLINES_CENTER("【　性技　】")
        FOR LCOUNT, 21, 50
            IF ABLNAME:LCOUNT != ""
                PRINTBUTTONLC @"%ABLNAME:LCOUNT%LV:{ABL:ARG:LCOUNT}", LCOUNT
                PCOUNT += 1
            ENDIF
            IF PCOUNT >= 5
                PRINTL
                PCOUNT = 0
            ENDIF
        NEXT
        PRINTL
        PCOUNT = 0
        CALL DRAWLINES_CENTER("【　精神　】")
        FOR LCOUNT, 51, 80
            IF ABLNAME:LCOUNT != ""
                PRINTBUTTONLC @"%ABLNAME:LCOUNT%LV:{ABL:ARG:LCOUNT}", LCOUNT
                PCOUNT += 1
            ENDIF
            IF PCOUNT >= 5
                PRINTL
                PCOUNT = 0
            ENDIF
        NEXT
    ENDIF
    PRINTL
    CUSTOMDRAWLINE -
    INPUT