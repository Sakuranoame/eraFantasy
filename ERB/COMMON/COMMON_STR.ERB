@SEX(ARG)
    #FUNCTIONS
    SELECTCASE CFLAG:ARG:2
        CASE 1
            RETURNF "男"
        CASE 2
            RETURNF "女"
        CASE 3
            RETURNF "扶她
        CASEELSE
            RETURNF "无"
    ENDSELECT

