@BREAST_CUP(ARG)
    #FUNCTIONS
    #DIMS BCUP
    SELECTCASE BASE:ARG:651
        CASE 63 TO 67
            BCUP = "65"
        CASE 68 TO 72
            BCUP = "70"
        CASE 73 TO 77
            BCUP = "75"
        CASE 78 TO 82
            BCUP = "80"
        CASE 83 TO 87
            BCUP = "85"
        CASE 88 TO 92
            BCUP = "90"
        CASE 93 TO 97
            BCUP = "95"
        CASE 98 TO 102
            BCUP = "100"
        CASE 103 TO 107
            BCUP = "105"
        CASE 108 TO 112
            BCUP = "110"
        CASE IS > 112
            BCUP = "112+"
    ENDSELECT
    SELECTCASE BASE:ARG:651 - BASE:ARG:652
        CASE 0
            BCUP += "ＡＡ"
        CASE 1
            BCUP += "Ａ"
        CASE 2
            BCUP += "Ｂ"
        CASE 3 
            BCUP += "Ｃ"
        CASE 4
            BCUP += "Ｄ"
        CASE 5
            BCUP += "Ｅ
        CASE 6
            BCUP += "Ｆ"
        CASE 7
            BCUP += "Ｇ"
        CASE 8
            BCUP += "Ｈ"
        CASE 9
            BCUP += "Ｉ"
        CASE 10
            BCUP += "Ｊ"
        CASE 11
            BCUP += "Ｋ"
        CASE 12
            BCUP += "Ｌ"
        CASE 13
            BCUP += "Ｍ"
        CASE 14
            BCUP += "Ｎ"
        CASE 15
            BCUP += "Ｏ"
        CASE 16
            BCUP += "Ｐ"
        CASE 17
            BCUP += "Ｑ"
        CASE 18
            BCUP += "Ｒ"
    ENDSELECT
    RETURNF BCUP

