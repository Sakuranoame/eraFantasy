@SHOPCHECK
IF RESULT == 900
    ALIGNMENT LEFT
    SAVEGAME
ELSEIF RESULT == 901
    ALIGNMENT LEFT
    LOADGAME
ELSEIF RESULT == 902
    CLEARLINE LINECOUNT
    CALL SETTING
ELSEIF RESULT == 200
    IF TARGET >= 1
        CALL TRAIN
        CLEARLINE LINECOUNT
    ELSE
        CALL CHARALIST
    ENDIF
ENDIF